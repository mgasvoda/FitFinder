# FitFinder Chainlit Configuration
# Copy this file to .env and customize the values

# Chainlit Authentication
CHAINLIT_AUTH_SECRET=your-chainlit-secret-key-here
CHAINLIT_ADMIN_USERNAME=admin
CHAINLIT_ADMIN_PASSWORD=fitfinder2024!

# Database
DATABASE_URL=sqlite:///./fitfinder.db

# AI/ML Service APIs
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional: Langfuse tracing (remove if not using)
# LANGFUSE_PUBLIC_KEY=your-langfuse-public-key
# LANGFUSE_SECRET_KEY=your-langfuse-secret-key

# Chainlit server settings (optional - defaults are usually fine)
# CHAINLIT_HOST=0.0.0.0
# CHAINLIT_PORT=8001 